<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
            
      <bean class="com.jspiders.sparingcore.beans.User" name = "user1">
      		<property name="id" value="1"/>
      		<property name="userName" value="Rushikesh"/>
      		<property name="email" value="rushikesh@gmail.com"/>
      		<property name="password" value="rushi@1234"/>
      
      </bean>
      
      <bean class="com.jspiders.sparingcore.beans.User" name="user2">
      		<property name="id" value="1"/>
      		<property name="userName" value="Ram"/>
      		<property name="email" value="ram@gmail.com"/>
      		<property name="password" value="ram@1234"/>
      
      </bean>
      
     <bean class="com.jspiders.sparingcore.beans.User" name="user3">
     	<constructor-arg name="id" value="1"/>
     	<constructor-arg name="userName" value="Seeta"/>
     	<constructor-arg name="email" value="seeta@gmail.com"/>
     	<constructor-arg name="password" value="seeta@123"/>
     	
     	
     </bean> 
</beans>